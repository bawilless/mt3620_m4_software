  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch for Azure Sphere Real-Time Applications (gdb)",
          "type": "azurespheredbg",
          "request": "launch",
          "args": [],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": true,
          "targetCore": "RTCore",
          "partnerComponents": [ "25025D2C-66DA-4448-BAE1-AC26FCDD3627", "CC0C53C7-E4B8-4D30-884C-034D6434DBBB" ],
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ]
      }
  ]
}